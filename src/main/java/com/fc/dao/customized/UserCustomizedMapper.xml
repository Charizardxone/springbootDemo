<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fc.dao.customized.UserCustomizedMapper">
    <insert id="insert">
        insert into user (name, age) values (#{name}, #{age})
    </insert>
    <update id="updateById">
        update user set name = #{name}, age = #{age} where id = #{id}
    </update>


    <select id="getById" resultType="com.fc.domain.SysUserEntity">
        select * from user where id = #{id}
    </select>
    <select id="findPage" resultType="com.fc.domain.SysUserEntity">

        select
        <if test="distinct">
            distinct
        </if>
        <include refid="com.fc.dao.SysUserEntityMapper.Base_Column_List" />
        from t_sys_user

    </select>
    <select id="findList" resultType="com.fc.domain.SysUserEntity">
        select
        <if test="distinct">
            distinct
        </if>
        <include refid="com.fc.dao.SysUserEntityMapper.Base_Column_List" />
        from t_sys_user
    </select>
</mapper>
